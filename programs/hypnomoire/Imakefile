INCLUDES = -I. -I../../exports/include -I../xcbtests
XCB=../../lib/XCB
XCBLIB=-L$(XCB) -lXCB
DEPXCBLIB=$(XCB)/libXCB.a

        DEPLIBS = $(DEPXCBLIB)
        XCBLIBS = $(XCBLIB) -L$(XLIB) -lXau

all:: hypnomoire

THREADS = -lpthread

hypnomoire_OBJS = hypnomoire.o ../xcbtests/reply_formats.o

NormalProgramTarget(hypnomoire,$(hypnomoire_OBJS),$(DEPXCBLIB),$(XCBLIBS),-lm $(THREADS))

DependTarget()
